{
  "name": "xac-sankey",
  "version": "1.0.4",
  "description": "X Atlas Consortia Sankey",
  "keywords": [
    "sankey",
    "data",
    "medical",
    "dbmi"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/x-atlas-consortia/data-sankey.git"
  },
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "watch": {
    "dist": "src/lib/js/*.js"
  },
  "scripts": {
    "dist": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "css": "stylus --include-css --compress -w ./src/styles/xac-sankey.styl -o ./src/lib/xac-sankey.css",
    "example": "serve ./dist",
    "dev": "serve ./src/lib -p 3003",
    "watch": "npm-watch",
    "all": "npm-run-all --parallel watch dev css"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "serve": "^14.2.4",
    "stylus": "^0.64.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-plugin-add-header-comment": "^1.0.3",
    "babel-plugin-file-loader": "^2.0.0"
  }
}
